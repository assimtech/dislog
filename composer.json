{
    "name": "assimtech/dislog",
    "type": "library",
    "description": "API call logger",
    "homepage": "https://github.com/assimtech/dislog",
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8",

        "symfony/options-resolver": "^3|^4|^5"
    },
    "require-dev": {
        "doctrine/common": "^2",
        "doctrine/orm": "^2",
        "guzzlehttp/psr7": "^1.7|^2",
        "psr/http-client": "^1",
        "psr/http-message": "^1",
        "phing/phing": "*",
        "phpspec/phpspec": "*",
        "psr/log": "^1",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "guzzlehttp/psr7": "Required if using LoggingHttpClient",
        "psr/http-client": "Required if using LoggingHttpClient",
        "psr/http-message": "Required if using LoggingHttpClient"
    },
    "autoload": {
        "psr-4": {
            "Assimtech\\Dislog\\": "src"
        }
    }
}
